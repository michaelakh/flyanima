<div class='container'>

<h1>City Airport Ita Codes</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>City</th>
      <th>Airport</th>
      <th>Country</th>
      <th>Searches</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @city_airport_ita_codes.each do |city_airport_ita_code| %>
      <tr>
        <td><%= city_airport_ita_code.code %></td>
        <td><%= city_airport_ita_code.city %></td>
        <td><%= city_airport_ita_code.airport %></td>
        <td><%= city_airport_ita_code.country %></td>
        <td><%= city_airport_ita_code.searches %></td>
        <td><%= link_to 'Show', city_airport_ita_code %></td>
        <td><%= link_to 'Edit', edit_city_airport_ita_code_path(city_airport_ita_code) %></td>
        <td><%= link_to 'Destroy', city_airport_ita_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
    
<div>
    <h3>Import a CSV File</h3>
      <%= form_tag import_city_airport_ita_codes_path, multipart: true do %>
        <%= file_field_tag :file %><br><br>
        <%= submit_tag "Import CSV" %>
      <% end %>
</div>

<br>

<%= link_to 'New City Airport Ita Code', new_city_airport_ita_code_path %>
</div>
